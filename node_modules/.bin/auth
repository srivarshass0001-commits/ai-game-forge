#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/project/sandbox/codebase/node_modules/.pnpm/@convex-dev+auth@0.0.86_@auth+core@0.37.4_convex@1.27.1_react@19.1.0__react@19.1.0/node_modules/@convex-dev/auth/dist/node_modules:/project/sandbox/codebase/node_modules/.pnpm/@convex-dev+auth@0.0.86_@auth+core@0.37.4_convex@1.27.1_react@19.1.0__react@19.1.0/node_modules/@convex-dev/auth/node_modules:/project/sandbox/codebase/node_modules/.pnpm/@convex-dev+auth@0.0.86_@auth+core@0.37.4_convex@1.27.1_react@19.1.0__react@19.1.0/node_modules/@convex-dev/node_modules:/project/sandbox/codebase/node_modules/.pnpm/@convex-dev+auth@0.0.86_@auth+core@0.37.4_convex@1.27.1_react@19.1.0__react@19.1.0/node_modules:/project/sandbox/codebase/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/project/sandbox/codebase/node_modules/.pnpm/@convex-dev+auth@0.0.86_@auth+core@0.37.4_convex@1.27.1_react@19.1.0__react@19.1.0/node_modules/@convex-dev/auth/dist/node_modules:/project/sandbox/codebase/node_modules/.pnpm/@convex-dev+auth@0.0.86_@auth+core@0.37.4_convex@1.27.1_react@19.1.0__react@19.1.0/node_modules/@convex-dev/auth/node_modules:/project/sandbox/codebase/node_modules/.pnpm/@convex-dev+auth@0.0.86_@auth+core@0.37.4_convex@1.27.1_react@19.1.0__react@19.1.0/node_modules/@convex-dev/node_modules:/project/sandbox/codebase/node_modules/.pnpm/@convex-dev+auth@0.0.86_@auth+core@0.37.4_convex@1.27.1_react@19.1.0__react@19.1.0/node_modules:/project/sandbox/codebase/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@convex-dev+auth@0.0.86_@auth+core@0.37.4_convex@1.27.1_react@19.1.0__react@19.1.0/node_modules/@convex-dev/auth/dist/bin.cjs" "$@"
else
  exec node  "$basedir/../.pnpm/@convex-dev+auth@0.0.86_@auth+core@0.37.4_convex@1.27.1_react@19.1.0__react@19.1.0/node_modules/@convex-dev/auth/dist/bin.cjs" "$@"
fi
